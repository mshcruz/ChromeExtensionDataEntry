{
  "manifest_version": 2,
  "name": "Chrome Extension Data Entry Demo",
  "version": "0.1.0",
  "description": "Demonstration of how to use Chrome extension to input data to a web form.",
  "icons": {
    "128": "media/logo128.png",
    "48": "media/logo48.png",
    "16": "media/logo16.png"
  },
  "permissions": [
    "activeTab",
    "https://script.google.com/*",
    "storage",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js; object-src 'self'",
  "browser_action": {
    "default_icon": "media/logo48.png",
    "default_title": "Chrome Extension Data Entry Demo"
  },
  "background": {
    "scripts": ["jquery-3.5.1.min.js", "background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*SampleForm.html*"],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ]
}
